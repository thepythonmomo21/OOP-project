public class EnergyDemand {
    // Total energy demand for the area or entity
    private double totalDemand;

    // Constructor
    public EnergyDemand(double totalDemand) {
        this.totalDemand = totalDemand;
    }

    // Method to calculate the energy demand
    public double calculateEnergyDemand() {
        // Calculation logic for energy demand
        // For demonstration purposes, let's assume a simple calculation
        // In a real-world scenario, this calculation would be more complex
        return totalDemand;
    }

    // Method to compare energy demand with energy production from the system
    public String compareWithProduction(double production) {
        if (totalDemand > production) {
            return "Energy demand exceeds production";
        } else if (totalDemand < production) {
            return "Energy production exceeds demand";
        } else {
            return "Energy production matches demand";
        }
    }

    // Method to adjust energy demand based on specific factors (e.g., changes in population or industry)
    public void adjustEnergyDemand(double factor) {
        totalDemand *= factor;
    }
}
